@model IdentityBase.Mvc.AccountMenuViewModel
@using ServiceBase
<ul class="nav nav-pills nav-stacked">
    @foreach (var item in Model.Items)
    {
        <li id="@item.Name">
            <a asp-route="@item.Route"
               asp-route-clientId="@Model.ClientId"
               asp-route-returnUrl="@Model.ReturnUrl">
                @Localizer[item.Name.HumanizeCamelCased().Capitalize()]
            </a>
        </li>
    }
</ul>
